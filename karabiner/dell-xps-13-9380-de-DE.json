      "description": "Map @ symbol to [Alt] + [Q]",
      "manipulators": [
        {
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": ["right_option"]
            }
          },
          "to": [
            {
              "key_code": "l",
              "modifiers": ["right_option"]
            }
          ],
          "type": "basic"
        },
        {
